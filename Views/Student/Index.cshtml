@model List<test.Src.Entity.Student>
<div class="container">
    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Contact No</th>
            <th>Class</th>
            <th>Faculty</th>
            <th>Action</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Address</td>
                <td>@item.ContactNo</td>
                <td>@item.ClassInfo.Name</td>
                <td>@item.Faculty.Name</td>
            </tr>
        }
        </tbody>
    </table>
</div>