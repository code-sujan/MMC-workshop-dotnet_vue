@model List<test.Src.Entity.Faculty>
<div class="container">
    <a asp-controller="Faculty" asp-action="Create" class="btn btn-primary">Create</a>
    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Description</th>
            <th>Action</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>
                   
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>
